<%- indent = @module_name.present? ? '  ' : '' -%>
<%= "module #{@module_name.camelize}\n" if @module_name.present? -%>
<%= indent %>class <%= class_name %>

  <%= indent %>def initialize
  <%= indent %>end

  <%= indent %>def call
  <%= indent %>end
<%= "\n#{indent}  private" if methods.present? %>
<%- for method in methods -%>

    <%= indent %>def <%= method %>
    <%= indent %>end
<%- end -%>
<%= indent %>end
<%= 'end' if @module_name.present? -%>